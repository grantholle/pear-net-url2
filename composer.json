{
    "name": "grantholle/pear-net-url2",
    "description": "A psr-4 compliant port of pear/net_url2: Class for parsing and handling URL. Provides parsing of URLs into their constituent parts (scheme, host, path etc.), URL generation, and resolving of relative URLs.",
    "type": "library",
    "keywords": [
        "pear",
        "net",
        "url",
        "uri",
        "networking",
        "rfc3986"
    ],
    "homepage": "https://github.com/grantholle/pear-net-url2",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Tom Klingenberg",
            "email": "tkli@php.net"
        },
        {
            "name": "David Coallier",
            "email": "davidc@php.net"
        },
        {
            "name": "Christian Schmidt",
            "email": "chmidt@php.net"
        },
        {
            "name": "Grant Holle",
            "homepage": "https://grantholle.com"
        }
    ],
    "support": {
        "issues": "https://github.com/grantholle/pear-net-url2/issues",
        "source": "https://github.com/grantholle/pear-net-url2"
    },
    "require": {
        "php": "^7.2|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Pear\\Net\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
